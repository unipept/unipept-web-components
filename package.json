{
  "name": "unipept-web-components",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "webpack && dts-bundle --configJson dts-bundle.config.json --emitOnIncludedFileNotFound",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^worker-loader!(.*)$": "$1"
    }
  },
  "files": [
    "src/**"
  ],
  "dependencies": {
    "@types/d3": "^5.7.2",
    "@types/d3-selection": "^1.4.1",
    "@types/jest": "^23.3.14",
    "@types/jquery": "^3.3.31",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "clipboard": "^2.0.4",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^3.2.0",
    "d3": "^5.11.0",
    "d3-selection": "^1.4.0",
    "jquery": "^3.4.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "schema-utils": "^2.2.0",
    "unipept-heatmap": "^1.0.26",
    "unipept-visualizations": "^1.7.3",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-fullscreen": "^2.1.5",
    "vue-property-decorator": "^8.1.0",
    "vue-style-loader": "^4.1.2",
    "vuetify": "^2.0.0",
    "vuex": "^3.0.1",
    "whatwg-fetch": "^3.0.0",
    "worker-loader": "^2.0.0",
    "workerize-loader": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@mdi/font": "^4.1.95",
    "@types/d3": "^5.7.2",
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "fibers": "^4.0.1",
    "file-loader": "^4.2.0",
    "jest": "^24.9.0",
    "jsdom-worker-fix": "^0.1.8",
    "nock": "^11.4.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
