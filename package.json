{
    "name": "unipept-web-components",
    "version": "1.2.2",
    "private": false,
    "types": "./types",
    "scripts": {
        "serve": "vue-cli-service build --target lib --name unipept-web-components src/index.ts --watch",
        "build": "vue-cli-service build --target lib --name unipept-web-components src/index.ts",
        "types": "tsc --emitDeclarationOnly && rm -rf types/test types/plugins types/main.d.ts",
        "test": "vue-cli-service test:unit",
        "lint": "vue-cli-service lint",
        "docs": "vue-styleguidist server",
        "docs:build": "vue-styleguidist build"
    },
    "main": "dist/unipept-web-components.common.js",
    "browser": "dist/unipept-web-components.umd.min.js",
    "unpkg": "dist/unipept-web-components.umd.min.js",
    "files": [
        "src/**",
        "types",
        "dist",
        "README.md"
    ],
    "dependencies": {
        "async": "^3.2.0",
        "axios": "^0.19.0",
        "canvg": "^3.0.5",
        "clipboard-polyfill": "^2.8.6",
        "convert-svg-to-png": "^0.5.0",
        "crypto-js": "^3.1.9-1",
        "d3": "^5.11.0",
        "d3-selection": "^1.4.0",
        "html-to-image-no-fonts": "^0.1.2",
        "html2canvas": "^1.0.0-rc.5",
        "jquery": "^3.4.1",
        "observable-fns": "^0.5.1",
        "shared-memory-datastructures": "0.1.8",
        "unipept-heatmap": "^1.0.27",
        "unipept-visualizations": "^1.7.3",
        "uuid": "^3.4.0",
        "vue": "^2.6.11",
        "vue-class-component": "^7.0.2",
        "vue-fullscreen": "^2.1.5",
        "vue-property-decorator": "^8.1.0",
        "vuetify": "^2.3.8",
        "vuex": "^3.0.1",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.11.1",
        "@babel/plugin-proposal-optional-chaining": "^7.11.0",
        "@babel/preset-env": "^7.6.3",
        "@mdi/font": "^4.1.95",
        "@types/async": "^3.2.3",
        "@types/d3": "^5.7.2",
        "@types/d3-selection": "^1.4.1",
        "@types/jest": "^26.0.14",
        "@types/jquery": "^3.3.31",
        "@types/uuid": "^3.4.7",
        "@vue/cli-plugin-babel": "^4.5.0",
        "@vue/cli-plugin-eslint": "^4.5.0",
        "@vue/cli-plugin-typescript": "^4.5.0",
        "@vue/cli-plugin-unit-jest": "^4.5.0",
        "@vue/cli-service": "^4.5.0",
        "@vue/eslint-config-typescript": "^4.0.0",
        "@vue/test-utils": "^1.1.0",
        "babel-core": "7.0.0-bridge.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.3.0",
        "babel-loader": "^8.1.0",
        "css-loader": "^4.2.1",
        "eslint": "^7.6.0",
        "eslint-plugin-vue": "^6.2.2",
        "file-loader": "^4.2.0",
        "flush-promises": "^1.0.2",
        "glob": "^7.1.6",
        "jest": "^26.4.2",
        "jsdom-worker-fix": "^0.1.8",
        "nock": "^11.4.0",
        "node-fetch": "^2.6.0",
        "node-sass": "^4.12.0",
        "less": "^3.12.2",
        "less-loader": "^7.0.1",
        "sass": "^1.17.4",
        "sass-loader": "^7.1.0",
        "svg-inline-loader": "^0.8.0",
        "ts-jest": "^26.4.0",
        "typescript": "3.8.3",
        "vue-cli-plugin-styleguidist": "^4.2.0",
        "vue-cli-plugin-vuetify": "^2.0.7",
        "vue-jest": "^3.0.5",
        "vue-style-loader": "^4.1.2",
        "vue-styleguidist": "^4.2.0",
        "vue-template-compiler": "^2.6.11",
        "vuepress": "^1.0.2",
        "vuetify-loader": "^1.6.0",
        "webpack-cli": "^3.3.7",
        "worker-loader": "^3.0.2"
    },
    "cdn": "dist/unipept-web-components.umd.min.js",
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "vue"
        ],
        "transform": {
            ".*\\.(vue)$": "vue-jest",
            "^.+\\.tsx?$": "ts-jest"
        },
        "testURL": "http://localhost/",
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1",
            "threads": "<rootDir>/src/test/mocks/MockThreads",
            "^worker-loader!./Pept2Data.worker.js$": "<rootDir>/src/test/MockPeptideContainerProcessor",
            "html-to-image-no-fonts": "<rootDir>/src/test/mocks/HtmlToImage"
        },
        "testEnvironment": "jsdom",
        "setupFilesAfterEnv": [
            "./src/test/init.js"
        ],
        "collectCoverageFrom": [
            "src/business/**/*.{ts,vue}",
            "src/components/**/*.{ts,vue}",
            ",!/node_modules/"
        ]
    }
}
